{
  "mcpServers": {
    "serena": {
      "type": "local",
      "command": [
        "uvx",
        "--from",
        "git+https://github.com/oraios/serena",
        "serena",
        "start-mcp-server",
        "--context",
        "ide-assistant"
      ],
      "enabled": true
    },
    "vibe-check": {
      "type": "local",
      "command": [
        "npx",
        "-y",
        "@pv-bhat/vibe-check-mcp",
        "start",
        "--stdio"
      ],
      "environment": {
        "GEMINI_API_KEY": "<YOUR_GEMINI_API_KEY>"
      },
      "enabled": true
    }
  }
}